# MySQL Database Configuration
# Copy this file to .env and update the values according to your MySQL setup

# Database name (default: bumi_kartanegara)
DB_NAME=bumi_kartanegara

# Database user (default: root)
DB_USER=root

# Database password (default: empty)
DB_PASSWORD=

# Database host (default: localhost)
DB_HOST=localhost

# Database port (default: 3306)
DB_PORT=3306

# Example for production:
# DB_NAME=staff_production
# DB_USER=staff_user
# DB_PASSWORD=your_secure_password
# DB_HOST=your_mysql_server.com
# DB_PORT=3306